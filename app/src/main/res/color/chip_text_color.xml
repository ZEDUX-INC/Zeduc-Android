<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:color="@color/color_white" android:state_enabled="true" android:state_selected="true"/>
    <item android:color="@color/color_white" android:state_enabled="true" android:state_checked="true"/>
    <item android:color="@color/color_white" android:state_enabled="true"/>

    <item android:color="@color/color_white"/>

    <!-- 87% opacity. -->
    <item android:alpha="0.87" android:color="@color/color_white" android:state_enabled="true"/>
    <!-- 38% of 87% opacity. -->
    <item android:alpha="0.33" android:color="@color/color_white"/>



</selector>

